{
  "name": "processor-interview",
  "scripts": {
    "dev": "bun run --hot server/index.ts",
    "format:write": "prettier . --write",
    "format:check": "prettier . --check",
    "lint:server": "cd server && eslint .",
    "lint:frontend": "cd frontend && eslint .",
    "start": "bun server/index.ts",
    "db:setup": "cd server && bun run scripts/setup-db.ts",
    "prisma:generate": "cd server && bunx prisma generate",
    "prisma:migrate": "cd server && bunx prisma migrate dev",
    "prisma:studio": "cd server && bunx prisma studio"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.5.0",
    "@kinde-oss/kinde-typescript-sdk": "^2.10.1",
    "@prisma/adapter-pg": "^6.7.0",
    "@prisma/client": "^6.7.0",
    "big.js": "^7.0.1",
    "decimal.js": "^10.5.0",
    "hono": "^4.7.8",
    "pg": "^8.15.6",
    "sax": "^1.4.1",
    "stream-chain": "^3.4.0",
    "stream-json": "^1.9.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.25.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/bun": "latest",
    "@types/sax": "^1.2.7",
    "@types/stream-json": "^1.7.8",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "typescript-eslint": "^8.31.1"
  }
}
